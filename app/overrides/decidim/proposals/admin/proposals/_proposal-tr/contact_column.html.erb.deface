<!-- insert_after "td:nth-of-type(10)" -->
<td>
    <% proposal.authors.each do |author| %>

      <% author_name = author.official_name_custom || author.name %>

      <% if author.telephone_number_custom.present? %>
        <%= author_name %> - <%= author.telephone_number_custom %>
      <% else %>
        <%= author_name %>
      <% end %>
      <br />

    <% end %>
</td>
